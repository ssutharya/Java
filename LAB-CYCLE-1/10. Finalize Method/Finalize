public class ABC {
    int a,b,c;
    
    ABC (int a, int b, int c){
        this.a = a;
        this.b = b;
        this.c = c;
    }
    public void print_product(){
        System.out.println("Product = " + (a * b * c));
    }
     public void finalize(){
        System.out.println("Garbage collection done!");
    }
}
